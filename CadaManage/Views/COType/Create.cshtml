@model CadaManage.Models.HandBookOfCOType

@{
    ViewBag.Title = "Create";
}

<h2>Создать тип</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        <div class="col-md-10">
            <b>Имя: </b>@Html.EditorFor(model => model.tHCOname, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.tHCOname, "", new { @class = "text-danger" })
        </div>
    </div>
    <a href="/COType/ShowHandBookOfCOType" class="btn btn-default btn-lg">&laquo; Смотреть справочнику типов</a>&emsp;
        <input type="submit" value="Сохранить" class="btn btn-default btn-lg" />
    </div>
}
